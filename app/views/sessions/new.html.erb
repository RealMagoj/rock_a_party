<h1 id="login_form_title">Log In</h1>
<%= form_tag sessions_path do %>
  <div>
    <span id="login_form"><%= label_tag :email %><br/></span>
    <%= text_field_tag :email %>
  </div>
  <div>
    <span id="login_form"><%= label_tag :password %><br/></span>
    <%= password_field_tag :password %>
  </div>
  <br>
  <div><%= submit_tag "Log In" %></div>
<% end %>